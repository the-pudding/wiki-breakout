#graphic .graphic__tracks
	width 100%
	position relative
	.track
		padding-left 2rem
		left 0
		// background blue
		position relative
	.timer
		position sticky
		top 50vh
		width 3rem
		height 3rem
		border-radius 50%
		border 1px solid $bg
		text-align center
		line-height 3rem
		margin 0
		padding 0
		font-family $mono
		background-color $yellow
		color $bg
		font-weight 600
		



